    ex:title "A UIMA Database Interface for Managing NLP-related Text Annotations-1" ;
    ex:writtenBy <http://www.gra.fo/schema/untitled-ekgauthorauthor/giuseppe_abrami@example.com> ;
    ex:year 2018 .

